{
  "name": "@pulsegraph/core",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".res.js",
  "bs-dependencies": ["@rescript/core"],
  "bsc-flags": ["-open RescriptCore"],
  "gentypeconfig": {
    "module": "es6",
    "moduleResolution": "node16",
    "generatedFileExtension": ".gen.ts",
    "shims": {
      "Date-RescriptCore": "Date",
      "Dict-RescriptCore": "Dict",
      "JSON-RescriptCore": "JSON",
      "Map-RescriptCore": "Map",
      "Null-RescriptCore": "Null"
    }
  }
}
